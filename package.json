{
  "name": "coreloops-cypress-pom-ts",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:report": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports,overwrite=false,html=false,json=true",
    "report:merge": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged.json",
    "report:html": "marge cypress/reports/merged.json --reportDir cypress/reports --reportFilename report.html",
    "test:ci": "npm run cy:run:report && npm run report:merge && npm run report:html"
  },
  "devDependencies": {
    "@types/mailparser": "^3.4.6",
    "cypress": "^13.0.0",
    "dotenv": "^17.2.3",
    "imapflow": "^1.2.8",
    "mailparser": "^3.9.3",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^5.4.5"
  }
}
