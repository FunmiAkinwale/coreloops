{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "types": ["cypress", "node"],
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["cypress/pages/*"]
    }
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts"]
}
